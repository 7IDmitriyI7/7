using System;

namespace _7._1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите значение угла в радианах 0<А<2*PI");
            double a = Convert.ToDouble(Console.ReadLine());
            if (0<a && a < 2 * Math.PI)
            {
                double rad = (Math.PI * a) / 180;
                Console.WriteLine("Угол  имеет  " + rad + "  градусов");
            }
            else
            {

                Console.WriteLine("Неверный ввод");
            }
           
        }
    }
}
